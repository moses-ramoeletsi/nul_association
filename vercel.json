{
    "version": 2,
    "builds": [
      { "src": "api/index.js", "use": "@vercel/node" },
      { "src": "Frontend/dist/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "destination": "/api" },
      { "src": "/(.*)", "destination": "/Frontend/dist/index.html" }
    ]
  }